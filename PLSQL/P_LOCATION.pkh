    psLOCT_CODE in P_BASE.tsLOCT_CODE := null,
    psCOUNTRY_CODE in P_BASE.tsLOC_COUNTRY_CODE := null,
    pdSTART_DATE in LOCATIONS.START_DATE%type := null,
    pdEND_DATE in LOCATIONS.END_DATE%type := null);
